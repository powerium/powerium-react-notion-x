{
  "name": "nextjs-notion-starter-kit",
  "version": "2.0.0",
  "private": true,
  "description": "The perfect starter kit for building beautiful websites with Next.js and Notion.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "repository": "transitive-bullshit/nextjs-notion-starter-kit",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel deploy",
    "deps": "run-s deps:*",
    "deps:update": "[ -z $GITHUB_ACTIONS ] && yarn add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && yarn link notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:link on CI'",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "run-p test:*",
    "test:lint": "eslint '**/*.{ts,tsx}'",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check"
  },
  "dependencies": {
    "@fisch0920/use-dark-mode": "^2.4.0",
    "@keyvhq/core": "^2.1.1",
    "@keyvhq/redis": "^2.1.4",
    "@react-icons/all-files": "^4.1.0",
    "@vercel/analytics": "^1.3.1",
    "@vercel/og": "^0.6.2",
    "classnames": "^2.3.1",
    "expiry-map": "^2.0.0",
    "fathom-client": "^3.7.0",
    "got": "^12.3.0",
    "isomorphic-unfetch": "^4.0.2",
    "lqip-modern": "https://github.com/powerium/lqip-modern.git#master",
    "next": "^14.2.5",
    "notion-client": "^6.16.0",
    "notion-types": "^6.15.6",
    "notion-utils": "^6.15.6",
    "p-map": "^7.0.2",
    "p-memoize": "^7.1.1",
    "posthog-js": "^1.147.0",
    "react": "^18.3.1",
    "react-body-classname": "^1.3.1",
    "react-dom": "^18.3.1",
    "react-notion-x": "^6.15.6",
    "react-tweet-embed": "^2.0.0",
    "react-use": "^17.4.2",
    "rss": "^1.2.2",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.5",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.16.1",
    "cross-env": "^7.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3"
  },
  "overrides": {
    "cacheable-request": {
      "keyv": "npm:@keyvhq/core@~1.6.6"
    }
  }
}
